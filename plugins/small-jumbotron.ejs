<div class="small-jumbotron" style="background: <%= tile['background-color'] %>">
	<div class="small-jumbotron-wrapper" style="color: <%= tile['text-color'] %>">
		<h2>
			<a href="<%= tile.href %>" style="color: <%= tile.color %>"><%= tile.name %></a>
		</h2>

		<p class="desc">
			<%= tile.desc %>
		</p>
	</div>
</div>
